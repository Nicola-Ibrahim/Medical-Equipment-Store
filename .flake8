[flake8]
ignore = C101, D403, F403, F401, F405, I100, I201, W503, W504, F821, Q000, E501, A001, A003, Q000, T201, I101
exclude =
    .git,
    __pycache__,
    .venv
filename = *.py
accept-encodings = utf-8
inline-quotes = single
max-complexity = 12
max-linenumber = 500
max-line-length = 120
multiline-quotes = double
per-file-ignores =
